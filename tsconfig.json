{
  "compilerOptions": {
    "noEmit": true,
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "rootDir": "./",
    "lib": ["es2017"],
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "graphql-compose-modules": ["./", "./src"]
    }
  },
  "include": ["src/**/*", "examples/**/*"]
}
